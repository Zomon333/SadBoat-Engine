<?xml version="1.0" encoding="utf-8"?>
<!--
    Some notes for the future:
    1) You can use the tags shown here to create comments that won't get parsed by RapidXML
    2-A) Tags with no children should take up one line; <Tag></Tag>
    2-B) Tags *with* children can take up numerous lines.
    3) If a tag will only contain one value, write it on one line. RapidXML *reads line breaks*!

-->
<!--
<ExampleMaterial>
     <Density>0000.00</Density> Use kg/m^3,
     <YungsModulous>000000000000.00</YungsModulous> Use Pascals,
     <CoeficientOfRestitution>0.00</CoeficientOfRestitution> half the true value
</ExampleMaterial>
-->
<SolidMaterials>
   
   <!--        
        Generic ceramics
   -->
   <Ceramics></Ceramics>
   
   <!--        
        Generic composites
   -->
   <Composites></Composites>

   <!--         
        Generic metals
   -->

   <Metals>
      <PureMetals>
          <Aluminum>
               <Density>2700</Density>
               <YungsModulous>000000000000.00</YungsModulous>
               <CoeficientOfRestitution>0.00</CoeficientOfRestitution>
          </Aluminum>
          <Copper>
               <Density>8940</Density>
               <YungsModulous>000000000000.00</YungsModulous>
               <CoeficientOfRestitution>0.00</CoeficientOfRestitution>
          </Copper>
          <Iron>
               <Density>7874</Density>
               <YungsModulous>000000000000.00</YungsModulous>
               <CoeficientOfRestitution>0.00</CoeficientOfRestitution>
          </Iron>
          <Lead>
               <Density>11340</Density>
               <YungsModulous>000000000000.00</YungsModulous>
               <CoeficientOfRestitution>0.00</CoeficientOfRestitution>
          </Lead>
          <Titanium>
               <Density>4507</Density>
               <YungsModulous>000000000000.00</YungsModulous>
               <CoeficientOfRestitution>0.00</CoeficientOfRestitution>
          </Titanium>
          <Tungsten>
               <Density>19250</Density>
               <YungsModulous>000000000000.00</YungsModulous>
               <CoeficientOfRestitution>0.00</CoeficientOfRestitution>
          </Tungsten>
      </PureMetals>
      <Alloys>
          <Aluminum></Aluminum>
          <Copper>
               <Brass></Brass>
               <Bronze>
                    <AluminumBronze></AluminumBronze>
                    <NickelAluminumBronze></NickelAluminumBronze>
               </Bronze>
          </Copper>
          <Iron>
               <AlloySteel></AlloySteel>
               <CarbonSteel></CarbonSteel>
               <CastIron></CastIron>
               <MildSteel></MildSteel>
               <StainlessSteel></StainlessSteel>
          </Iron>
          <Titanium></Titanium>
      </Alloys>
   </Metals>
   
   <!--        
        Generic organics
   -->
   <Organics></Organics>

   <!--        
        Generic Polymers
   -->
   <Polymers></Polymers>
   
   <!--        
        Development only materials
   -->
   <Dev>
      <BedRock></BedRock><!-- Infinite Density -->
      <Unobtanium></Unobtanium><!-- Infinite FYFM -->
   </Dev>
</SolidMaterials>
